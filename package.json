{
  "name": "express-graphql-boilerplate",
  "version": "0.1.0",
  "description": "Just a boilerplate for starting graphQL API projects in node with mongodb",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-register",
    "dev": "nodemon server --exec babel-node",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "start": "npm run build && node dist/index.js",
    "lint": "./node_modules/.bin/eslint server"
  },
  "author": "Francisco Calder√≥n",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphiql": "^0.11.10",
    "graphql": "^0.11.7",
    "mongoose": "^4.13.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "lint"
  ]
}
